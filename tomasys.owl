        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
    </owl:DatatypeProperty>
    


    <!-- http://metacontrol_unexmin#fg_performance -->

    <owl:DatatypeProperty rdf:about="http://metacontrol_unexmin#fg_performance">
